{
    "name": "agentic_ml_pipeline",
    "type": "batch",
    "description": "8-Step JTBD ML Pipeline: Define → Locate → Prepare → Confirm → Execute → Monitor → Modify → Conclude",
    "uuid": "agentic_ml_pipeline_v1",
    "blocks": [
        {
            "uuid": "load_training_data",
            "name": "load_training_data",
            "type": "data_loader",
            "language": "python",
            "downstream_blocks": [
                "prepare_h2o_frame"
            ],
            "jtbd_step": "2_locate",
            "configuration": {
                "full_refresh": false
            }
        },
        {
            "uuid": "prepare_h2o_frame",
            "name": "prepare_h2o_frame",
            "type": "transformer",
            "language": "python",
            "upstream_blocks": [
                "load_training_data"
            ],
            "downstream_blocks": [
                "validate_data_quality"
            ],
            "jtbd_step": "3_prepare"
        },
        {
            "uuid": "validate_data_quality",
            "name": "validate_data_quality",
            "type": "sensor",
            "language": "python",
            "upstream_blocks": [
                "prepare_h2o_frame"
            ],
            "downstream_blocks": [
                "train_automl"
            ],
            "jtbd_step": "4_confirm"
        },
        {
            "uuid": "train_automl",
            "name": "train_automl",
            "type": "custom",
            "language": "python",
            "upstream_blocks": [
                "validate_data_quality"
            ],
            "downstream_blocks": [
                "evaluate_leaderboard"
            ],
            "jtbd_step": "5_execute",
            "configuration": {
                "max_runtime_secs": 3600,
                "max_models": 20
            }
        },
        {
            "uuid": "evaluate_leaderboard",
            "name": "evaluate_leaderboard",
            "type": "custom",
            "language": "python",
            "upstream_blocks": [
                "train_automl"
            ],
            "downstream_blocks": [
                "feedback_loop"
            ],
            "jtbd_step": "6_monitor"
        },
        {
            "uuid": "feedback_loop",
            "name": "feedback_loop",
            "type": "custom",
            "language": "python",
            "upstream_blocks": [
                "evaluate_leaderboard"
            ],
            "downstream_blocks": [
                "deploy_mojo"
            ],
            "jtbd_step": "7_modify"
        },
        {
            "uuid": "deploy_mojo",
            "name": "deploy_mojo",
            "type": "data_exporter",
            "language": "python",
            "upstream_blocks": [
                "feedback_loop"
            ],
            "jtbd_step": "8_conclude"
        }
    ],
    "variables": {
        "target_column": "target",
        "problem_type": "classification",
        "primary_metric": "AUC",
        "performance_threshold": 0.80,
        "max_runtime_secs": 3600,
        "max_models": 20
    },
    "schedule": {
        "cron": "0 2 * * *",
        "timezone": "UTC"
    }
}